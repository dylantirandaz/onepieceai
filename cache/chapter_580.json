      "*": "<div class=\"mw-content-ltr mw-parser-output\" lang=\"en\" dir=\"ltr\"><p>\n<aside role=\"region\" class=\"portable-infobox pi-background pi-border-color pi-theme-Chapter pi-layout-default\">\n<h2 class=\"pi-item pi-item-spacing pi-title pi-secondary-background\" data-source=\"title\">The War's Conclusion</h2>\n\n<figure class=\"pi-item pi-image\" data-source=\"image\">\n\t<a href=\"https://static.wikia.nocookie.net/onepiece/images/d/dc/Chapter_580.png/revision/latest?cb=20130121043930\" class=\"image image-thumbnail\" title=\"Chapter 580\">\n\t\t<img src=\"https://static.wikia.nocookie.net/onepiece/images/d/dc/Chapter_580.png/revision/latest/scale-to-width-down/268?cb=20130121043930\" srcset=\"https://static.wikia.nocookie.net/onepiece/images/d/dc/Chapter_580.png/revision/latest/scale-to-width-down/268?cb=20130121043930 1x, https://static.wikia.nocookie.net/onepiece/images/d/dc/Chapter_580.png/revision/latest/scale-to-width-down/535?cb=20130121043930 2x\" class=\"pi-image-thumbnail\" data-relevant=\"1\" alt=\"Chapter 580\" width=\"270\" height=\"437\" data-image-key=\"Chapter_580.png\" data-image-name=\"Chapter 580.png\" />\n\t\t\n\t</a>\n\t\n</figure>\n<section class=\"pi-item pi-group pi-border-color\">\n\t<h2 class=\"pi-item pi-header pi-secondary-font pi-item-spacing pi-secondary-background\">Chapter Info</h2>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"vol\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Volume:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\"><a href=\"/wiki/Volume_59\" title=\"Volume 59\">59</a></div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"chapter\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Chapter:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\">580</div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"jname\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Japanese Title:</