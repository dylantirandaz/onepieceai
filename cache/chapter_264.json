      "*": "<div class=\"mw-content-ltr mw-parser-output\" lang=\"en\" dir=\"ltr\"><p>\n<aside role=\"region\" class=\"portable-infobox pi-background pi-border-color pi-theme-Chapter pi-layout-default\">\n<h2 class=\"pi-item pi-item-spacing pi-title pi-secondary-background\" data-source=\"title\">Warrior Kamakiri vs. God Enel</h2>\n\n<figure class=\"pi-item pi-image\" data-source=\"image\">\n\t<a href=\"https://static.wikia.nocookie.net/onepiece/images/7/7c/Chapter_264.png/revision/latest?cb=20130509032001\" class=\"image image-thumbnail\" title=\"Chapter 264\">\n\t\t<img src=\"https://static.wikia.nocookie.net/onepiece/images/7/7c/Chapter_264.png/revision/latest/scale-to-width-down/268?cb=20130509032001\" srcset=\"https://static.wikia.nocookie.net/onepiece/images/7/7c/Chapter_264.png/revision/latest/scale-to-width-down/268?cb=20130509032001 1x, https://static.wikia.nocookie.net/onepiece/images/7/7c/Chapter_264.png/revision/latest/scale-to-width-down/535?cb=20130509032001 2x\" class=\"pi-image-thumbnail\" data-relevant=\"1\" alt=\"Chapter 264\" width=\"270\" height=\"412\" data-image-key=\"Chapter_264.png\" data-image-name=\"Chapter 264.png\" />\n\t\t\n\t</a>\n\t\n</figure>\n<section class=\"pi-item pi-group pi-border-color\">\n\t<h2 class=\"pi-item pi-header pi-secondary-font pi-item-spacing pi-secondary-background\">Chapter Info</h2>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"vol\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Volume:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\"><a href=\"/wiki/Volume_28\" title=\"Volume 28\">28</a></div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"chapter\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Chapter:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\">264</div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"jname\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Japanese