      "*": "<div class=\"mw-content-ltr mw-parser-output\" lang=\"en\" dir=\"ltr\"><p>\n<aside role=\"region\" class=\"portable-infobox pi-background pi-border-color pi-theme-Chapter pi-layout-default\">\n<h2 class=\"pi-item pi-item-spacing pi-title pi-secondary-background\" data-source=\"title\">Adventure in the Land of Mystery</h2>\n\n<figure class=\"pi-item pi-image\" data-source=\"image\">\n\t<a href=\"https://static.wikia.nocookie.net/onepiece/images/9/97/Chapter_1127.png/revision/latest?cb=20250404053210\" class=\"image image-thumbnail\" title=\"Chapter 1127\">\n\t\t<img src=\"https://static.wikia.nocookie.net/onepiece/images/9/97/Chapter_1127.png/revision/latest/scale-to-width-down/268?cb=20250404053210\" srcset=\"https://static.wikia.nocookie.net/onepiece/images/9/97/Chapter_1127.png/revision/latest/scale-to-width-down/268?cb=20250404053210 1x, https://static.wikia.nocookie.net/onepiece/images/9/97/Chapter_1127.png/revision/latest/scale-to-width-down/536?cb=20250404053210 2x\" class=\"pi-image-thumbnail\" data-relevant=\"1\" alt=\"Chapter 1127\" width=\"270\" height=\"424\" data-image-key=\"Chapter_1127.png\" data-image-name=\"Chapter 1127.png\" />\n\t\t\n\t</a>\n\t\n</figure>\n<section class=\"pi-item pi-group pi-border-color\">\n\t<h2 class=\"pi-item pi-header pi-secondary-font pi-item-spacing pi-secondary-background\">Chapter Info</h2>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"vol\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Volume:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\"><a href=\"/wiki/Volume_111\" title=\"Volume 111\">111</a></div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"chapter\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-font\">Chapter:</h3>\n\t\n\t<div class=\"pi-data-value pi-font\">1127</div>\n</div>\n\n<div class=\"pi-item pi-data pi-item-spacing pi-border-color\" data-source=\"jname\">\n\t\n\t\t<h3 class=\"pi-data-label pi-secondary-